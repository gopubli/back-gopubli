{
    "name": "gopubli/back-gopubli",
    "type": "project",
    "description": "GoPubLi API Backend",
    "keywords": ["laravel", "framework", "api"],
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "brick/math": "^0.14.1",
        "carbonphp/carbon-doctrine-types": "^3.2",
        "dflydev/dot-access-data": "^3.0.3",
        "doctrine/inflector": "^2.0.10",
        "doctrine/lexer": "^3.0.1",
        "dragonmantank/cron-expression": "^3.4",
        "egulias/email-validator": "^4.0.2",
        "fakerphp/faker": "^1.23.1",
        "fruitcake/php-cors": "^1.3.0",
        "graham-campbell/result-type": "^1.1.3",
        "guzzlehttp/guzzle": "^7.9.2",
        "guzzlehttp/promises": "^2.0.4",
        "guzzlehttp/psr7": "^2.7.0",
        "guzzlehttp/uri-template": "^1.0.3",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10",
        "league/commonmark": "^2.5.3",
        "league/config": "^1.2.0",
        "league/flysystem": "^3.29.1",
        "league/flysystem-local": "^3.29.0",
        "league/mime-type-detection": "^1.16.0",
        "monolog/monolog": "^3.8.1",
        "nesbot/carbon": "^3.8.2",
        "nette/schema": "^1.3.2",
        "nette/utils": "^4.0.5",
        "nikic/php-parser": "^5.3.1",
        "nunomaduro/termwind": "^2.2.0",
        "phpoption/phpoption": "^1.9.3",
        "psr/container": "^2.0.2",
        "psr/event-dispatcher": "^1.0.0",
        "psr/http-client": "^1.0.3",
        "psr/http-factory": "^1.1.0",
        "psr/http-message": "^2.0",
        "psr/log": "^3.0.2",
        "psr/simple-cache": "^3.0.0",
        "psy/psysh": "^0.12.4",
        "ramsey/collection": "^2.0.0",
        "ramsey/uuid": "^4.7.6",
        "symfony/console": "^7.1.8",
        "symfony/css-selector": "^7.1.6",
        "symfony/error-handler": "^7.1.8",
        "symfony/event-dispatcher": "^7.1.6",
        "symfony/finder": "^7.1.6",
        "symfony/http-foundation": "^7.1.8",
        "symfony/http-kernel": "^7.1.8",
        "symfony/mailer": "^7.1.6",
        "symfony/mime": "^7.1.6",
        "symfony/polyfill-ctype": "^1.31.0",
        "symfony/polyfill-intl-grapheme": "^1.31.0",
        "symfony/polyfill-intl-idn": "^1.31.0",
        "symfony/polyfill-intl-normalizer": "^1.31.0",
        "symfony/polyfill-mbstring": "^1.31.0",
        "symfony/polyfill-php80": "^1.31.0",
        "symfony/polyfill-php83": "^1.31.0",
        "symfony/polyfill-uuid": "^1.31.0",
        "symfony/process": "^7.1.8",
        "symfony/routing": "^7.1.6",
        "symfony/service-contracts": "^3.5.0",
        "symfony/string": "^7.1.8",
        "symfony/translation": "^7.1.6",
        "symfony/translation-contracts": "^3.5.0",
        "symfony/uid": "^7.1.6",
        "symfony/var-dumper": "^7.1.8",
        "tijsverkoyen/css-to-inline-styles": "^2.2.7",
        "vlucas/phpdotenv": "^5.6.1",
        "voku/portable-ascii": "^2.0.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "laravel/pint": "^1.18.3",
        "laravel/sail": "^1.37",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.5.0",
        "pestphp/pest": "^3.5",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
